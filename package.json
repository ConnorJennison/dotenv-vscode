{
  "name": "dotenv-vscode",
  "displayName": "Dotenv & Dotenv Vault Official",
  "description": "Official Dotenv & Dotenv Vault Extension",
  "author": "Mot @dotenvorg",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/dotenv-org/dotenv-vscode",
  "icon": "dotenv.png",
  "sponsor": {
    "url": "https://dotenv.org/pricing"
  },
  "bugs": {
    "url": "https://github.com/dotenv-org/dotenv-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotenv-org/dotenv-vscode.git"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dotenv.login",
    "onCommand:dotenv.logout",
    "onCommand:dotenv.new",
    "onCommand:dotenv.open",
    "onCommand:dotenv.pull",
    "onCommand:dotenv.push",
    "onCommand:dotenv.status",
    "onCommand:dotenv.versions",
    "onCommand:dotenv.whoami"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dotenv.login",
        "title": "dotenv login"
      },
      {
        "command": "dotenv.logout",
        "title": "dotenv logout"
      },
      {
        "command": "dotenv.new",
        "title": "dotenv new"
      },
      {
        "command": "dotenv.open",
        "title": "dotenv open"
      },
      {
        "command": "dotenv.pull",
        "title": "dotenv pull"
      },
      {
        "command": "dotenv.push",
        "title": "dotenv push"
      },
      {
        "command": "dotenv.status",
        "title": "dotenv status"
      },
      {
        "command": "dotenv.versions",
        "title": "dotenv versions"
      },
      {
        "command": "dotenv.whoami",
        "title": "dotenv whoami"
      }
    ]
  },
  "scripts": {
    "lint": "standard --fix",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "npm run lint && vsce package",
    "install-package": "code --install-extension",
    "login": "vsce login",
    "publish": "npm install && npm run lint && vsce package && vsce publish"
  },
  "publisher": "dotenv",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "standard": "^17.0.0",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0"
  },
  "keywords": [
    "dotenv",
    "dotenv-vault",
    "environment variables",
    "vault",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings",
    "official"
  ]
}
