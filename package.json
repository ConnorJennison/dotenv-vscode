{
  "name": "dotenv-vscode",
  "displayName": "Official Dotenv | Vault",
  "description": "Official Dotenv Vault. Sync your .env files, with VSCode.",
  "author": "Mot @motdotla",
  "version": "0.5.2",
  "license": "MIT",
  "homepage": "https://github.com/dotenv-org/dotenv-vscode",
  "icon": "dotenv.png",
  "galleryBanner": {
    "color": "#3E1661",
    "theme": "dark"
  },
  "qna": false,
  "bugs": {
    "url": "https://github.com/dotenv-org/dotenv-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotenv-org/dotenv-vscode.git"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Languages",
    "Formatters",
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [
    "onCommand:dotenv.login",
    "onCommand:dotenv.logout",
    "onCommand:dotenv.new",
    "onCommand:dotenv.open",
    "onCommand:dotenv.pull",
    "onCommand:dotenv.push",
    "onCommand:dotenv.status",
    "onCommand:dotenv.versions",
    "onCommand:dotenv.whoami",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dotenv",
        "aliases": [
          ".env",
          "env",
          "environment variables",
          "secrets",
          "vault"
        ],
        "extensions": [
          ".env.vault",
          ".env.me",
          ".flaskenv",
          ".env",
          ".env-sample",
          ".env.example",
          ".env.local",
          ".env.dev",
          ".env.dev.local",
          ".env.build",
          ".env.build.local",
          ".env.development",
          ".env.development.local",
          ".env.test",
          ".env.test.local",
          ".env.testing",
          ".env.qa",
          ".env.qa.local",
          ".env.uat",
          ".env.uat.local",
          ".env.stag",
          ".env.stag.local",
          ".env.stage",
          ".env.stage.local",
          ".env.staging",
          ".env.staging.local",
          ".env.live",
          ".env.live.local",
          ".env.production",
          ".env.production.local",
          ".env.prod",
          ".env.prod.local",
          ".env.previous",
          ".env.staging.previous",
          ".env.production.previous"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dotenv",
        "scopeName": "source.dotenv",
        "path": "./syntaxes/dotenv.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "dotenv.login",
        "title": "dotenv login"
      },
      {
        "command": "dotenv.logout",
        "title": "dotenv logout"
      },
      {
        "command": "dotenv.new",
        "title": "dotenv new"
      },
      {
        "command": "dotenv.open",
        "title": "dotenv open"
      },
      {
        "command": "dotenv.pull",
        "title": "dotenv pull"
      },
      {
        "command": "dotenv.push",
        "title": "dotenv push"
      },
      {
        "command": "dotenv.status",
        "title": "dotenv status"
      },
      {
        "command": "dotenv.versions",
        "title": "dotenv versions"
      },
      {
        "command": "dotenv.whoami",
        "title": "dotenv whoami"
      }
    ]
  },
  "scripts": {
    "lint": "standard --fix",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "npm run lint && vsce package",
    "install-package": "code --install-extension",
    "login": "vsce login",
    "publish": "npm install && npm run lint && vsce package && vsce publish"
  },
  "publisher": "dotenv",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "standard": "^17.0.0",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0"
  },
  "keywords": [
    "dotenv",
    "dotenv-vault",
    "environment variables",
    "vault",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings",
    "official",
    "development",
    "productivity"
  ],
  "dependencies": {
    "dotenv": "^16.0.1"
  }
}
